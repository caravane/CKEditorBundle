<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="caravane_ck_editor.twig.trim_asset_version_extension.class">Caravane\CKEditorBundle\Twig\TrimAssetVersionTwigExtension</parameter>
    </parameters>

    <services>
        <service
            id="caravane_ck_editor.twig.trim_asset_version_extension"
            class="%caravane_ck_editor.twig.trim_asset_version_extension.class%"
            public="false"
        >
            <tag name="twig.extension" />
        </service>
    </services>
</container>
